<VBox alignment="CENTER" prefHeight="394.0" prefWidth="323.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.TicTacToeController">
