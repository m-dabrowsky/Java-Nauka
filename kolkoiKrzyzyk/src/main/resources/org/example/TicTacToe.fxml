          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <GridPane gridLinesVisible="true" prefHeight="302.0" prefWidth="337.0" VBox.vgrow="NEVER">
